[package]
name = "{{PACKAGE_NAME}}"
version = "0.2.0"
authors = ["Swagger Codegen team and contributors"]
edition = "2018"

[dependencies]
serde = "1.0"
serde_derive = "1.0"
serde_yaml = "0.7"
serde_json = "1.0"
base64 = "~0.7.0"
futures = "*"
hyper = "0.13.4"
url = "2.1.1"
headers = "*"
tokio = { version = "0.2", features = ["full"] }
bigdecimal = { version = "*", features = ["serde"] }
chrono = { version = "*", features = ["serde"] }

[dev-dependencies]
tokio-core = "*"
tokio = { version = "*", features = ["full"] }
